{
  "manifest_version": 2,
  "name": "Talkyng Screensharing",
  "description": "A simple extension that allows you to share your desktop into Talkyng meetings.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://meet.jit.si/*"
      ]
  }
}
